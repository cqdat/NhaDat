@using bds.Models
@model BannerViewModel

<div id="ban_left" class="ban_scroll" style="display: block;top: 0px;position: fixed;left: 1px;">
    <div class="adPosition" positioncode="BANNER_POSITION_LEFT" style="margin-bottom: 0px;">
        @foreach (var q in Model.BannerLeft)
        {
            <div class="adshared" style="margin-bottom:5px">
                <div class="adshareditem aditem" time="15" style="display: block;" src="~/Areas/Cpanel/Images/Banners/@q.ImageURL" altsrc="https://file4.batdongsan.com.vn/2018/01/11/vou5EgPQ/20180111135042-0eac.jpg" 
                     bid="6292" tip="" tp="7" w="100" h="300" k="">
                    <a href="@q.LinkBanner" target="@q.KieuChuyenHuong" title="@q.NameBanner" rel="nofollow">
                    <img src="~/Areas/Cpanel/Images/Banners/@q.ImageURL" style="max-width: 100%; height:300px;">
                    </a>
                </div>
            </div>
        }
        

    </div>
</div>
